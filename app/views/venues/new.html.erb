<h2>Create a New Venue</h2>

<div id="new-venue">
  <%= turbo_frame_tag 'new-venue' do %>
    <%= form_with model: @venue, url: venues_path, local: true, method: :post do |form| %>
      <div class="field">
        <%= form.label :name %>
        <%= form.text_field :name %>
      </div>
      <div class="field">
        <%= form.label :location %>
        <%= form.text_field :location %>
      </div>
      <!-- Add other fields as needed -->
      <div class="actions">
        <%= form.submit 'Create Venue' %>
      </div>
    <% end %>
  <% end %>
</div>














